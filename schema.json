{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Glazbeni festivali",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "festival_id": {
        "type": "number"
      },
      "naziv_festivala": {
        "type": "string"
      },
      "država": {
        "type": "string"
      },
      "grad": {
        "type": "string"
      },
      "datum_početka": {
        "type": "string"
      },
      "datum_završetka": {
        "type": "string"
      },
      "cijena_ulaznica": {
        "type": "number"
      },
      "žanrovi_glazbe": {
        "type": "string"
      },
      "organizator": {
        "type": "string"
      },
      "web_stranica": {
        "type": "string"
      },
      "izvođači": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "ime_izvodjaca": {
              "type": "string"
            }
          },
          "required": [
            "ime_izvodjaca"
          ]
        }
      }
    },
    "required": [
      "festival_id",
      "naziv_festivala",
      "država",
      "grad",
      "datum_početka",
      "datum_završetka",
      "cijena_ulaznica",
      "žanrovi_glazbe",
      "organizator",
      "web_stranica",
      "izvođači"
    ]
  }
}